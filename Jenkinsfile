pipeline {
    agent any

    stages {
        stage('Info') {
            steps {
                echo "INFO STAGE"
                echo "Running on Jenkins node"
            }
        }

        stage('Build') {
            steps {
                echo "BUILD STAGE"
            }
        }

        stage('Test') {
            steps {
                echo "TEST STAGE"
            }
        }
    }
}
